  "bugs": {
    "url": "https://github.com/abhi886/algo-app/issues"
  },
  "homepage": "https://github.com/abhi886/algo-app#readme",
  "dependencies": {
    "express": "^4.17.1",
    "joi": "^17.4.0",
    "mongoose": "^5.11.10",
    "nodemon": "^2.0.6"
  },
  "lint-staged": {
    "*.js": "eslint --cache --fix",
    "*.{js,css,md}": "prettier --config .prettierrc --write"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "devDependencies": {
    "@babel/cli": "^7.13.0",
    "@babel/core": "^7.13.8",
